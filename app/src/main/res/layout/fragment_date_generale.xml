<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="Fragments.DateGeneraleFragment"
    android:background="#FFFFFF">


        <TextView
            android:id="@+id/dg1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg1"
            android:layout_below="@+id/dg1"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg2"
            android:layout_below="@+id/dg2"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg3"
            android:layout_below="@+id/dg3"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg4"
            android:layout_below="@+id/dg4"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg5"
            android:layout_below="@+id/dg5"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg6"
            android:layout_below="@+id/dg6"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg7"
            android:layout_below="@+id/dg7"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg8"
            android:layout_below="@+id/dg8"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg9"
            android:layout_below="@+id/dg9"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>

        <TextView
            android:id="@+id/dg11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dg10"
            android:layout_below="@+id/dg10"
            android:layout_marginTop="15dp"
            android:theme="@style/Text"/>


    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="-5dp"
        android:weightSum="3"
        >
    <Button
        android:id="@+id/flow_date_gen"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Date generale"
        android:layout_weight="1"
        android:background="#005AA0"/>

    <Button
        android:id="@+id/flow_clienti"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Client"
        android:layout_weight="1"
        android:background="#0173B9"/>

    <Button
        android:id="@+id/flow_furnizor"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Furnizor"
        android:layout_weight="1"
        android:background="#0173B9"/>
</LinearLayout>

</RelativeLayout>
