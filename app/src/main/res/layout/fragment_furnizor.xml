<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Fragments.FurnizorFragment"
    android:background="#FFFFFF">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="40dp"
        android:textStyle="bold"
        android:layout_centerInParent="true"
        android:id="@+id/textView18" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="62dp"
        android:text="Nume companie"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="51dp"
        android:text="Cod inregistrare" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView14"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="41dp"
        android:text="Cod fiscal" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Banca"
        android:layout_above="@+id/textView18"
        android:layout_alignStart="@+id/textView15" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cod IBAN"
        android:layout_below="@+id/textView18"
        android:layout_alignStart="@+id/textView19" />

    <CheckedTextView
        android:id="@+id/checkedTextView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView7"
        android:layout_marginStart="69dp"
        android:layout_toEndOf="@+id/textView7"
        android:text="CheckedTextView" />

    <CheckedTextView
        android:id="@+id/checkedTextView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/checkedTextView8"
        android:layout_alignTop="@+id/textView14"
        android:text="CheckedTextView" />

    <CheckedTextView
        android:id="@+id/checkedTextView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/checkedTextView13"
        android:layout_alignTop="@+id/textView15"
        android:text="CheckedTextView" />

    <CheckedTextView
        android:id="@+id/checkedTextView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/checkedTextView14"
        android:layout_alignTop="@+id/textView19"
        android:text="CheckedTextView" />

    <CheckedTextView
        android:id="@+id/checkedTextView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignEnd="@+id/checkedTextView15"
        android:text="CheckedTextView" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView20"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="60dp"
        android:text="Adresa" />

    <CheckedTextView
        android:id="@+id/checkedTextView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/checkedTextView16"
        android:layout_alignTop="@+id/textView21"
        android:text="CheckedTextView" />

</RelativeLayout>
